! execute xrdb ~/.Xresources after editing the file

! st.font: xft:mononoki Nerd Font:size=9
st.font: mononoki Nerd Font Mono-11

st.chscale: 1.3
st.alpha: 0.9

! dark+ theme (sort of)
! black
st.color0: #000000
st.color8: #1C1C1C
! red
st.color1: #DD6666
st.color9: #DB8877
! green
st.color2: #559944
st.color10: #00995C
! yellow
st.color3: #DDBB77
st.color11: #DDDDAA
! blue
st.color4: #114477
st.color12: #3399DD
! magenta
st.color5: #662277
st.color13: #CC88CC
! cyan
st.color6: #2277AA
st.color14: #5FD7FF
! white
st.color7: #A8A8A8
st.color15: #FFFFFF

! dmenu
dmenu.font: mononoki Nerd Font Mono-9
dmenu.background: #1C1C1C
dmenu.foreground: #A8A8A8
dmenu.selbackground: #00995C
dmenu.selforeground: #FFFFFF
dmenu.selbackground-hl: #00804D

! sxiv
Sxiv.background: #1C1C1C
Sxiv.font: mononoki Nerd Font Mono-9

! nsxiv
Nsxiv.window.background: #1C1C1C
Nsxiv.bar.background: #000000
Nsxiv.bar.foreground: #333333
Nsxiv.bar.font: mononoki Nerd Font Mono-9
!Nsxiv.window.alpha: 0.5 ! alpha patch not applied --TODO
